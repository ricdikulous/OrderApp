<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns="http://java.sun.com/xml/ns/javaee" version="2.5">
  <filter>
        <filter-name>ObjectifyFilter</filter-name>
        <filter-class>com.googlecode.objectify.ObjectifyFilter</filter-class>
  </filter>
  <filter-mapping>
        <filter-name>ObjectifyFilter</filter-name>
        <url-pattern>/*</url-pattern>
  </filter-mapping>
   <servlet>
        <servlet-name>SystemServiceServlet</servlet-name>
        <servlet-class>com.google.api.server.spi.SystemServiceServlet</servlet-class>
        <init-param>
            <param-name>services</param-name>
            <param-value>
                com.example.ric.myapplication.backend.RegistrationEndpoint,
                com.example.ric.myapplication.backend.MessagingEndpoint,
                com.example.ric.myapplication.backend.api.MenuEndpoint,
                com.example.ric.myapplication.backend.api.OrderEndpoint,
            </param-value>
        </init-param>
    </servlet>
    <servlet>
        <servlet-name>UploadMenuItem</servlet-name>
        <servlet-class>com.example.ric.myapplication.backend.servlet.UploadMenuItem</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DeleteMenuItem</servlet-name>
        <servlet-class>com.example.ric.myapplication.backend.servlet.DeleteMenuItem</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>PayPalServlet</servlet-name>
        <servlet-class>com.example.ric.myapplication.backend.servlet.PayPalServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>MessageServlet</servlet-name>
        <servlet-class>com.example.ric.myapplication.backend.servlet.MessageServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ChannelCloseServlet</servlet-name>
        <servlet-class>com.example.ric.myapplication.backend.servlet.ChannelCloseServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ChannelCloseServlet</servlet-name>
        <url-pattern>/channelclose</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>MessageServlet</servlet-name>
        <url-pattern>/message</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SystemServiceServlet</servlet-name>
        <url-pattern>/_ah/spi/*</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>UploadMenuItem</servlet-name>
        <url-pattern>/uploadmenuitem</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DeleteMenuItem</servlet-name>
        <url-pattern>/deletemenuitem</url-pattern>
    </servlet-mapping>


    <servlet-mapping>
        <servlet-name>PayPalServlet</servlet-name>
        <url-pattern>/paypal</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>UpdateTypesServlet</servlet-name>
        <servlet-class>com.example.ric.myapplication.backend.servlet.menu.types.UpdateTypesServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>UpdateTypesServlet</servlet-name>
        <url-pattern>/updatetypes</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>RemoveTypeServlet</servlet-name>
        <servlet-class>com.example.ric.myapplication.backend.servlet.menu.types.RemoveTypeServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>RemoveTypeServlet</servlet-name>
        <url-pattern>/removetype</url-pattern>
    </servlet-mapping>


    <welcome-file-list>
        <welcome-file>index.html</welcome-file>
    </welcome-file-list>
</web-app>